  },
  "homepage": "https://github.com/shzlw/paramplate#readme",
  "devDependencies": {
    "@types/node": "^16.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "rimraf": "^3.0.2"
  },
  "dependencies": {}
